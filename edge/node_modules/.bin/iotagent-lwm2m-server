#!/bin/sh
basedir=`dirname "$0"`

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../lwm2m-node-lib/bin/iotagent-lwm2m-server.js" "$@"
  ret=$?
else 
  node  "$basedir/../lwm2m-node-lib/bin/iotagent-lwm2m-server.js" "$@"
  ret=$?
fi
exit $ret
